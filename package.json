    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:deploy": "prisma migrate deploy"
  },
  "dependencies": {
    "@fastify/busboy": "^2.1.1",
    "@prisma/client": "^6.18.0",
    "dayjs": "^1.11.13",
    "next": "^14.2.13",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.34",
    "@types/react-dom": "^18.2.15",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.13",
    "prettier": "^3.3.3",
    "prisma": "^6.18.0",
    "typescript": "^5.6.3"
  }
}
